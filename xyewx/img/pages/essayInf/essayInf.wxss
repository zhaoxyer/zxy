.inf image{
  display: inline-block;
  vertical-align: top;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%; 
  margin-left: 10px;
}
.inf view{
   margin-left: 10px;
   display: inline-block;
  vertical-align: top;
  line-height: 25px;
  font-size:14px
}
.inf view text{
  display: block
}
.container{padding: 0;align-items: initial;color:#666666;}
.cont{font-size: 14px;line-height: 25px;border-top:1px solid  #efeff4;padding: 0 10px;}
.list{border-top:1px solid  #EAEAEF;border-bottom:1px solid  #efeff4;margin-top:5px;}
.infnum view{display: inline-block;vertical-align: top;width:33%;line-height:60rpx;text-align: center;border-right:1px solid  #efeff4; }
.infnum view:last-child{border-right: none;}
.infnum{border-top:1px solid  #efeff4;font-size:12px;}
.tittle{text-indent: 10px;font-size:14px;border-top:1px solid  #efeff4;line-height:30px;  }
.addessay{width:60px;height: 60px;border-radius: 50%;border: 2px solid gray;position: fixed;bottom: 100rpx;right:10px;line-height:60px;text-align: center; font-size: 50px;color: gray;font-weight: bold;}
.comlist{
    padding: 0 10px;
    font-size: 14px;
    line-height: 30px;
}
.com{
   border-bottom: 1px solid  #efeff4;
   font-size: 14px;
   width: 100%;
   line-height: 30px;
}
.pushcom button{
    width: 50px;
    font-size: 14px;
    padding: 0;
    margin: 0;
    right: 10px;
    position: absolute;
    line-height: 50px;
    background: none;
    border: none;
    color: #333333
}
.quxiao{
    right: 50px!important;
}
.pushcom{
    border: 1px solid  #efeff4;
    border-radius: 10px;
    right: 10px;
    left:10px;
    height: 200px;
    position: absolute;
    z-index: 999;
    bottom: 20px;
    background:#F8F8F8;
}
button::after{
    border: none;
}
.comlist text{display: inline-block;vertical-align: top}
.button-hover[type="primary"] {
color:#333333;
background-color:#F8F8F8;
}



